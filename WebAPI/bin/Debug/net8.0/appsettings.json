{
    "Logging": {
        "LogLevel": {
            "Default": "Information",
            "Microsoft.AspNetCore": "Warning"
        }
    },
    "AllowedHosts": "*",
    "ConnectionStrings": {
        "TenderAutoAppContext": "${DATABASE_CONNECTION_STRING}"
    },
    "MailSettings": {
        "Host": "${MAIL_HOST}",
        "Port": "${MAIL_PORT}",
        "EnableSsl": true,
        "SenderEmail": "${MAIL_SENDER}",
        "UserName": "${MAIL_USERNAME}",
        "Password": "${MAIL_PASSWORD}"
    },
    "Jwt": {
        "Key": "${JWT_KEY}"
    }
}
